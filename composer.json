{
  "name": "coolonce/logger-interface",
  "description": "This package allows you to send data for logging to the HTTP daemon associated with the clickhouse",
  "keywords": [
    "logging",
    "log"
  ],
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "Kiselev Vadim",
      "email": "coolonce@yandex.ru"
    }
  ],
  "minimum-stability": "stable",
  "require": {
    "guzzlehttp/guzzle": "^6.3",
    "laravel/framework": ">=5.6",
  },
  "require-dev": {
    "guzzlehttp/guzzle": "^6.3",
    "phpunit/phpunit": "^8"
  },
  "extra": {
    "laravel": {
      "providers": [
        "Logger\\LoggerServiceProvider"
      ]
    }
  },
  "autoload": {
    "psr-4": {
      "Logger\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  }
}
